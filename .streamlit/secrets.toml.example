# Streamlit Cloud Secrets Configuration Example
# Copy this file to secrets.toml and fill in your actual values
# DO NOT commit secrets.toml to git

[gcp]
# Your Google Cloud Project ID
project_id = "your-project-id-here"

# Your service account credentials (JSON format as string)
# Paste the entire content of your service-account-key.json file here
credentials = '''
{
  "type": "service_account",
  "project_id": "your-project-id",
  "private_key_id": "key-id-here",
  "private_key": "-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n",
  "client_email": "clip-search-logger@your-project.iam.gserviceaccount.com",
  "client_id": "your-client-id",
  "auth_uri": "https://accounts.google.com/o/oauth2/auth",
  "token_uri": "https://oauth2.googleapis.com/token",
  "auth_provider_x509_cert_url": "https://www.googleapis.com/oauth2/v1/certs",
  "client_x509_cert_url": "https://www.googleapis.com/robot/v1/metadata/x509/clip-search-logger%40your-project.iam.gserviceaccount.com"
}
'''

# Alternative: Minimal configuration
# If you prefer to use individual fields instead of the full JSON
[gcp_minimal]
project_id = "your-project-id"
private_key = "-----BEGIN PRIVATE KEY-----\nYOUR_PRIVATE_KEY_HERE\n-----END PRIVATE KEY-----\n"
client_email = "clip-search-logger@your-project.iam.gserviceaccount.com" 